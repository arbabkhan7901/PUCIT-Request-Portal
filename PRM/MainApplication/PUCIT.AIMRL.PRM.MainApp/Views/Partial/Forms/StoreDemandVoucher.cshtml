@{
    Layout = null;
}


<div class="widget-body" id="storedemandvoucher" style="display:none">
    <div class="row">
        <div class="space-4"></div>
        <div style="padding-left:50px;">
            <div class="col-md-3">
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label for="form-field-username"><b>Name: </b> </label>
                    <label id="name" for="form-field-username">@ViewBag.Name</label>
                </div>
            </div>
            <div class="col-md-3">
                <label for="form-field-first"><b>Designation: </b></label>&nbsp;
                <label id="rollNo" class="text" for="form-field-first">@ViewBag.title</label>
            </div>
            <div class="col-md-3">
            </div>
            <div class="space-4"></div>
            <div class="space-4"></div>
        </div>
    </div>
    <div class="page-content">
        <div class="page-header">
            <div class="space-4"></div>
            <div class="row">
                <div style="padding-left:50px;">
                    <div class="col-md-3">
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:45px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label class="asterisk" for="form-field-username"><b> Select Items: </b> </label>
                            <div id="store_item_result"></div>
                            <select class="input-sm form-control" id="Store_Voucher_items" autofocus>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label class="asterisk" for="form-field-username"><b>Quantity: </b></label>
                        <div id="store_quantity"></div>
                        <input type="text" id="storedemandQuantity" style="width:60px;height:30px;" maxlength="5" />&emsp;&emsp;
                        <button id="addStoreItem" style="width: 60px; height: 30px;">
                            @*<i class="ace-icon fa fa-check"></i>*@
                            Add
                        </button>
                    </div>
                    <br>
                </div>
            </div>
            <div class="space-4">
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-12" style="padding-left:29%">
                            <div class="col-sm-6">
                                <label for="form-field-first"><b> Items Selected: </b></label>
                                <div id="storevoucherTableResult"></div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group">
                        <div class="col-sm-12 col-xs-12 col-sm-9 " style="padding-left:30%">
                            <div id="draggable" style="max-height:250px;overflow-y:auto">
                                <table id="storevoucherTable" class="sortabletable  table table-bordered">
                                    <thead class="thin-border-bottom">
                                        <tr>
                                            <th>Item Name</th>
                                            <th>Quantity</th>
                                            <th>Remove Item</th>
                                        </tr>
                                    </thead>
                                    <tbody></tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:50px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-6">
                        <div class="form-group">
                            <label for="form-field-username"><b>Purpose: </b></label>
                            <div id="general_reason_result1"></div>
                            <textarea id="general_reason1" class="form-control" maxlength="200" rows="6"></textarea>
                        </div>
                    </div>
                </div>
            </div>

            <div class="space-4"></div>
            <div class="row">
                <div style="padding-left:50px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <input type="radio" id="Regular1" name="option" />
                            <label for="form-field-signature"><b> Regular Budget  </b> </label>
                            <div id="store_budget"></div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 1. Material & Equipment </b> </label>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 2. Maint. Of Computer </b> </label>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 3. Furniture </b> </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 4. Telephone </b> </label>
                        </div>
                    </div>
                </div>

            </div>
            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 5. Electronic Mail Service </b> </label>
                        </div>
                    </div>
                </div>
            </div>


            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 6. Stationary And Printing </b> </label>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 7. Library </b> </label>
                        </div>
                    </div>
                </div>
            </div>

            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 8. Contingencies </b> </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div style="padding-left:80px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <label for="form-field-signature"><b> 9. Entertainment </b> </label>
                        </div>
                    </div>
                </div>
            </div>

            <div class="space-4"></div>
            <div class="row">
                <div style="padding-left:50px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <input type="radio" id="Self1" name="option" />
                            <label for="form-field-signature"><b> Self-Suport  </b> </label>
                        </div>
                    </div>
                </div>
            </div>
            <div class="space-4"></div>
            <div class="row">
                <div style="padding-left:50px;">
                    <div class="col-md-3">
                    </div>
                    <div class="col-md-3">
                        <div class="form-group">
                            <input type="radio" id="CollegeFund1" name="option" />
                            <label for="form-field-signature"><b> College Fund  </b> </label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
