@{
    Layout = null;

}
<div id="demand_Voucher" style="display:none">
    <div class="row">
        <div class="col-xs-12 col-sm-7" style="padding-left:10%">
            <div>
                <span class="blue"><label><b>Diary No. :</b></label></span>
                <label id="DiaryNo16"></label>
            </div>
            <div class="space-4"></div>
            <div>
                <span class="blue"><label><b>Designation:</b></label></span>
                <label id="rollno16"></label>
            </div>
        </div>

        <div class="col-xs-12 col-sm-5">
            <div>
                <span class="blue"><label><b>Date:</b></label></span>
                <label id="mydate16"></label>
            </div>
            <div class="space-4"></div>
            <div>
                <span class="blue"><label><b>Name :</b></label></span>
                <label id="myname16"></label>
            </div>
        </div>
    </div>
    <div class="space-4"></div>

    <div class="row">
        <div class="col-xs-12 col-sm-7" style="padding-left:10%">

            <div class="space-4"></div>
            <div id="VoucherData">
                <table class="table table-bordered " style="width:350px">
                    <thead class="thin-border-bottom">
                        <tr>
                            <th>
                                Item Name
                            </th>
                            <th>
                                Quantity
                            </th>
                        </tr>
                    </thead>
                    <tbody id="VoucherTable"></tbody>
                </table>
            </div>
        </div>
        <div class="col-xs-12 col-sm-7" style="padding-left:10%">
            <div class="space-4"></div>
            <span class="blue">
                <label><b>Budget :</b></label>

            </span>
            <p id="budgetDisplay"> </p>
        </div>

    </div>

</div>