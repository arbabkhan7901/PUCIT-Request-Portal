@{
    Layout = null;
}
<div id="SemTranscript" style="display:none">
    <div class="row">
        <div class="col-xs-12 col-sm-7" style="padding-left:10%">
            <div>
                <span class="blue"><label><b>Diary No. :</b></label></span>
                <label id="DiaryNo11"></label>
            </div>
            <div class="space-4"></div>
            <div>
                <span class="blue"><label><b>Roll No. :</b></label></span>
                <label id="rollno11"></label>
            </div>
        </div>

        <div class="col-xs-12 col-sm-5">
            <div>
                <span class="blue"><label><b>Date:</b></label></span>
                <label id="mydate11"></label>
            </div>
            <div class="space-4"></div>
            <div>
                <span class="blue"><label><b>Name :</b></label></span>
                <label id="myname11"></label>
            </div>
        </div>
    </div>
    <div class="space-4"></div>
    <div class="row">
        <div class="col-xs-6 col-sm-7" style="padding-left:10%">
            <div class="widget-box" style="border:none;">
                <div>
                    <span class="blue"><label> <b> Semester    :   </b></label></span>
                    <label id="Currentsemester"></label>
                </div>
                <div class="space-4"></div>
                <div>
                    <span class="blue"> <label><b> Transcript required for semester(s) :   </b></label></span>
                    <label id="targetsemester"></label>
                </div>
                <div class="space-4"></div>
                <div>
                    <span class="blue"><label> <b> Challan Form No.  :   </b></label></span>
                    <label id="challanNumber"></label>
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-5">
            <div class="widget-box" style="border:none;">
                <div>
                    <span class="blue"><label> <b> Section    :   </b></label></span>
                    <label id="section"></label>
                </div>
                <div class="space-4"></div>
                <div>
                    <span class="blue"><label><b>  Father's Name   :   </b> </label> </span>
                    <label id="fathername">    </label>
                </div>
            </div>
        </div>
    </div>
</div>

