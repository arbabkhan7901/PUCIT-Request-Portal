@{
    Layout = "~/Views/Shared/_HeaderLayout.cshtml";
}
@section Header
{
  
    @Scripts.Render("~/bundles/ContactUsReport")
}
@section scripts
{
    <script type="text/javascript">
        $(document).ready(function () {
            MyWebApp.UI.ContactUsReport.readyMain();
        });
    </script>
}
@section BreadCrumb
{
    <li>
        <a href='@Url.Content("~/Reports/")'>Reports</a>
    </li>
    <li>
        <a href='@Url.Content("~/Reports/ContactUsReport/")'>Contact Us Report</a>
    </li>
}

<div class="page-content">
    <div class="page-header">
        <h1>Contact Us Report</h1>
           <div class="row">
                <div class="col-xs-12">
                    <div class="row">
                        <div class="col-sm-12">
                            <div class="col-sm-3">
                                <label class="lbl">Name</label>
                                <input id="name" type="text" class="input-sm form-control" placeholder="Type Name">
                            </div>
                            <div class="col-sm-3">
                                <label class="lbl">Email</label>
                                <input id="email" type="text" class="input-sm form-control" placeholder="Type Email">
                            </div>
                            <div class="col-sm-2">
                                <label class="lbl">Start Date</label>
                                <input id="sdate" class="input-sm form-control" type="date" data-date-format="dd-mm-yyyy">
                            </div>
                            <div class="col-sm-2">
                                <label class="lbl">End Date</label>
                                <input id="edate" class="input-sm form-control" type="date" data-date-format="dd-mm-yyyy">
                            </div>
                            <div class="col-sm-1" style="padding-top: 15px">
                                <a role="button" class="pull-right btn-new-mail" data-toggle="modal" id="search">
                                    <span class="btn btn-purple no-border">
                                        <i class="menu-icon fa fa-search"></i>
                                        <span class="bigger-110">Search</span>
                                    </span>
                                </a>
                            </div>

                        </div>
                    </div>
                   
                 
                </div>
            </div>
    </div>
    <!-- /.page-header -->
    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->
            <div class="row">
                        <div class="col-sm-10">
                            <div class="col-sm-2">
                                Page Size:
                                <select id="cmbPageSizeSearch">
                                    <option value="10" selected>10</option>
                                    <option value="20">20</option>
                                    <option value="30">30</option>
                                </select>

                            </div>
                            <div class="col-sm-2">
                                <span id="spResultsFound"></span>
                            </div>
                           @* <div class="col-sm-6">
                                <ul class="pagination pull-right no-margin"></ul>
                            </div>*@
                        </div>
                    </div>

            <div class="row">
                <div class="col-xs-12">
                    <div class="message-list-container">
                        <div class="message-list" id="LoginHistory">
                            <table id="simple-table6" class="table  table-bordered table-hover"></table>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.row -->
            <div class="hr hr-18 dotted hr-double"></div>
            <div class="modal-footer no-margin-top">
                <ul class="pagination pull-right no-margin">
                  
                </ul>
            </div>
            <!-- /.modal-content -->
        </div>
    </div>
    <!-- PAGE CONTENT ENDS -->
</div>
<!-- main.cont -->
@section PopupsAndTemplates
{
    <script id="ContactUsReportTemplate" type="text/x-handlebars-template">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Machine IP</th>
                <th>Description</th>
                <th>Time</th>
            </tr>
        </thead>
        <tbody>
            {{#each ContactUsList}}
            <tr>
                 <td>{{Name}}</td>
                <td>{{Email}}</td>
                <td>{{MachineIP}}</td>
                <td>{{Description}}</td>
                <td>{{EntryDateTimeStr}}</td>
            </tr>
            {{/each}}
        </tbody>
    </script>
}